doctype html
html
  head
    style(type='text/css').
      html, body { height: 100%; margin: 0; padding: 0; }
      #map { height: 100%; }
  body
    #map
    script(type='text/javascript').
      var map;
      function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: -34.397, lng: 150.644},
      zoom: 8
      });
      }
    script(async='', defer='', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyD42cn8u5aAfqjgkMwWYYdTWtCRRaw8ZwY&callback=initMap')
